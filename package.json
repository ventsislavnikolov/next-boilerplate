  "scripts": {
    "test": "jest",
    "prepare": "husky",
    "build": "next build",
    "start": "next start",
    "test:ci": "jest --ci",
    "dev": "next dev --turbo",
    "test:watch": "jest --watch",
    "prettier": "prettier --check",
    "e2e:ui": "playwright test --ui",
    "lint-staged": "pnpx lint-staged",
    "e2e:headless": "playwright test",
    "test:coverage": "jest --coverage",
    "test-storybook": "test-storybook",
    "prettier:write": "prettier --write",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "analyze": "ANALYZE=true next build",
    "env:pull": "npx dotenv-vault@latest pull",
    "env:push": "npx dotenv-vault@latest push",
    "commitlint": "npx --no -- commitlint --edit",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "lint": "npx oxlint --import-plugin --jest-plugin --jsx-a11y-plugin --nextjs-plugin -D all -c ./.eslintrc.json --deny-warnings"
  },
