  "homepage": "https://github.com/ventsislavnikolov/next-boilerplate",
  "bugs": {
    "url": "https://github.com/ventsislavnikolov/next-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ventsislavnikolov/next-boilerplate.git"
  },
  "author": {
    "name": "Ventsislav Nikolov"
  },
  "scripts": {
    "test": "jest",
    "build": "next build",
    "start": "next start",
    "test:ci": "jest --ci",
    "dev": "next dev --turbo",
    "test:watch": "jest --watch",
    "e2e:ui": "playwright test --ui",
    "lint-staged": "pnpx lint-staged",
    "e2e:headless": "playwright test",
    "test:coverage": "jest --coverage",
    "test-storybook": "test-storybook",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "analyze": "ANALYZE=true next build",
    "env:pull": "npx dotenv-vault@latest pull",
    "env:push": "npx dotenv-vault@latest push",
    "commitlint": "npx --no -- commitlint --edit",
    "format": "prettier --check ./src/**/*.{ts,tsx,css}",
    "format:write": "prettier --write ./src/**/*.{ts,tsx,css}",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "lint": "npx oxlint --import-plugin --jest-plugin --jsx-a11y-plugin --nextjs-plugin -D all -A nursery -c ./.eslintrc.json --deny-warnings"
  },
